<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
@using WeddingPlanner.Models
@model Wedding

<partial name="NavPartial"></partial>
<h1 class="text-center my-3">@Model.WedderOne and @Model.WedderTwo's Wedding</h1>
<div class="row my-3">
    <div class="col">
        <h3>@Model.Ceremony</h3>
    </div>
    <div class="col">
        <h4>@Model.Address</h4>
    </div>
</div>
<div class="row my-2">
    <div class="col">
        <h2>Guests List</h2>
        <hr>
        @foreach (Relation name in @Model.Guests)
        {
            <h5>@name.Guest.FirstName @name.Guest.LastName</h5>
        }
    </div>
    <div class="col"></div>
</div>